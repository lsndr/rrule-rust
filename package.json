{
  "name": "rrule-rust",
  "version": "1.1.5",
  "main": "index.js",
  "types": "index.d.ts",
  "keywords": [
    "rrule",
    "icalendar",
    "rust",
    "rs"
  ],
  "description": "RRule implementation for Node.js written in Rust",
  "repository": "lsndr/rrule-rust",
  "author": {
    "name": "lsndr",
    "email": "alexander@lsndr.me",
    "url": "https://lsndr.me"
  },
  "napi": {
    "name": "rrule-rust",
    "package": {
      "name": "@rrule-rust/lib"
    },
    "triples": {
      "additional": [
        "aarch64-apple-darwin",
        "aarch64-linux-android",
        "aarch64-unknown-linux-gnu",
        "aarch64-unknown-linux-musl",
        "aarch64-pc-windows-msvc",
        "armv7-unknown-linux-gnueabihf",
        "x86_64-unknown-linux-musl",
        "x86_64-unknown-freebsd",
        "i686-pc-windows-msvc",
        "armv7-linux-androideabi",
        "universal-apple-darwin"
      ]
    }
  },
  "license": "MIT",
  "devDependencies": {
    "@napi-rs/cli": "^2.15.1",
    "@semantic-release/git": "^10.0.1",
    "@types/jest": "^29.5.0",
    "@types/node": "^20.11.16",
    "@typescript-eslint/eslint-plugin": "^5.50.0",
    "@typescript-eslint/parser": "^5.50.0",
    "benny": "^3.7.1",
    "eslint": "^8.33.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "^29.5.0",
    "lint-staged": "^13.1.0",
    "pre-commit": "^1.2.2",
    "prettier": "^2.8.3",
    "rrule": "^2.7.2",
    "semantic-release": "^22.0.12",
    "ts-jest": "^29.0.5",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.4"
  },
  "pre-commit": [
    "lint:fix-staged"
  ],
  "lint-staged": {
    "**/*.{js,ts,json}": "eslint --fix --max-warnings=0",
    "**/*.rs": "rustfmt"
  },
  "engines": {
    "node": ">= 10"
  },
  "scripts": {
    "artifacts": "napi artifacts",
    "build": "napi build --platform --release --js-package-name @rrule-rust/lib",
    "build:debug": "napi build --platform",
    "prepublishOnly": "napi prepublish -t npm",
    "test": "jest",
    "universal": "napi universal",
    "version": "napi version",
    "benchmark": "ts-node ./benchmark/index.ts",
    "lint": "cargo fmt --all -- --check && eslint \"**/*.{js,ts,json}\" --max-warnings=0",
    "lint:fix": "cargo fmt --all && yarn lint --fix",
    "lint:fix-staged": "lint-staged"
  },
  "dependencies": {
    "conventional-changelog-conventionalcommits": "^7.0.2"
  }
}
